<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Single Care Test Suite">
  <test name="Single Care TEST SUITE">
    <method-selectors>
      <method-selector>
        <script language="beanshell">
          <![CDATA[testngMethod.getMethodName().equalsIgnoreCase("Sign Up - Verify user can sign up a account")]]>
        </script>
      </method-selector>
    </method-selectors>
    <parameter name="env.properties" value="resources/web"/>
    <parameter name="scenario.file.loc" value="scenarios/web/signup.bdd"/>
    <classes>
      <class name="com.qmetry.qaf.automation.step.client.text.BDDTestFactory"/>
    </classes>
  </test> <!-- Single Care TEST SUITE -->
</suite> <!-- Single Care Test Suite -->
